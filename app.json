{
  "expo": {
    "name": "Aora",
    "slug": "aora",
    "scheme": "aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assetss/images/logo-small.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/small.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.ashish.aora"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-small.png",
        "backgroundColor": "#161622"
      },
      "package": "com.ashish.aora",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "80d12803-211f-478b-9dbe-0c77a5f8599a"
      }
    }
  }
}